<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Bison / Flex"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="bison"
			DisplayName="Bison"
			CommandLine="c:\cygwin\bin\bison -d -v -t -ocore\$(InputName).cpp [inputs]"
			Outputs="core\$(InputName).cpp; core\$(InputName).hh"
			FileExtensions="*.yy"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="Flex"
			DisplayName="Flex"
			CommandLine="c:\cygwin\bin\flex -ocore\$(InputName).cpp [inputs]"
			Outputs="$(InputName).cpp"
			FileExtensions="*.ll"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
